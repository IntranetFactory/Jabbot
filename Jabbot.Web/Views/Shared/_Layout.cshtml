<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet">
    <style>body {padding-top: 60px;}</style>
    <link href="@Url.Content("~/Content/bootstrap-responsive.min.css")" rel="stylesheet">
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <link href="@Url.Content("~/Content/base.css")" rel="stylesheet" type="text/css" />
    <link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>
    <script type="text/javascript">
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-31291305-2']);
        _gaq.push(['_trackPageview']);
        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>
</head>
<body>
    <div class="container">
        <div id="content">
            @RenderBody()
        </div>
    </div>
    <hr />
    <div class="container">
        <p>&nbsp;</p>
        <div id="footer" class="row-fluid">
            <div class="span10">
                <p>
                    <strong class="logo-font">Jabbot</strong> Inspired by <a href="http://hubot.github.com/">Hubot</a> and <a href="https://github.com/davidfowl/Jabbot">Jabbot</a> <small>(the original)</small><br />
